{
  "name": "djs-protofy",
  "version": "0.8.0",
  "description": "A library created to make Discord.JS easier to use through prototypes.",
  "main": "out/index.js",
  "types": "index.d.ts",
  "scripts": {
    "watch": "tsc -watch",
    "test:node": "node --test out/test",
    "test": "npm run test:ts && npm run build && npm run test:node",
    "test:ts": "tsc --noEmit",
    "build": "tsc",
    "prepublish": "npm run build",
    "release": "npm run prepublish && npm publish"
  },
  "keywords": [
    "discord.js",
    "prototypes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/astro-bunker/djs-protofy.git"
  },
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "default": "./out/index.js",
      "import": "./out/index.mjs"
    },
    "./init": {
      "default": "./init.js",
      "import": "./init.mjs"
    }
  },
  "dependencies": {
    "source-map-support": "^0.5.21",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "discord.js": "^14.14.1",
    "eslint": "^8.57.0",
    "gen-esm-wrapper": "^1.1.3",
    "typescript": "^5.4.3"
  }
}